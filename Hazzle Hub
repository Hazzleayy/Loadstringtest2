ocal Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Hazzle Hub", "Synapse")
local Tab = Window:NewTab("Brookhaven")
local Section = Tab:NewSection("Brookhaven")
Section:NewButton("IceHub", "idk", function()
loadstring(game:HttpGet('https://icehub.cf/IceHubLoader'))()
    print("Clicked")
end)
local Tab = Window:NewTab("Doors")
local Section = Tab:NewSection("Doors")
Section:NewButton("Darkrai X", "Doors hub", function()    loadstring(game:HttpGet("https://raw.githubusercontent.com/GamingScripter/Darkrai-X/main/Games/Doors"))()
    print("Clicked")
end)
Section:NewButton("Entities Spawner", "Not FE", function() loadstring(game:HttpGet('https://raw.githubusercontent.com/plamen6789/DoorsEntitySummonerGUI/main/EntityGUI'))()
    print("Clicked")
end)
Section:NewButton("Doors Script Hub", "got Vynixius Hub,Darkrai X,Ominoux Hub,Entity Spawner Hub..", function() loadstring(game:HttpGet("https://raw.githubusercontent.com/UltraStuff/scripts2/main/doors"))()
    print("Clicked")
end)
